{
    "compilerOptions": {
      /* Docs: https://www.typescriptlang.org/tsconfig */
          "baseUrl": ".",
      "target": "ES2020",
      "module": "commonjs",
      "outDir": "./built",
      "rootDir": ".",
      "strict": true,
      "checkJs": true,
      "allowJs": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "moduleResolution": "node",
      "types": ["node"],
      "lib": ["es6", "dom"],
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "allowUnreachableCode": false,
      "noFallthroughCasesInSwitch": true,
      "useUnknownInCatchVariables": false, // TypeScript 4.4+ only!
      "typeRoots": ["./node_modules/@types"],
      "paths": {
              "@discordjs/voice": ["../../"],
              "@discordjs/opus": ["./node_modules/@discordjs/opus"],
              "sodium": ["./node_modules/sodium"],
              "libsodium-wrappers": ["./node_modules/libsodium-wrappers"],
              "tweetnacl": ["./node_modules/tweetnacl"],
              "*": ["node_modules/*", "src/types/*"]
          }
    },
    "include": ["./**/*.ts", "./**/*.js", "src/commands/message-events/ai-image", "src/commands/message-events/warframe", "src/commands/message-events/warframe", "src/commands/message-events/ai-complete"],
    "exclude": ["node_modules", "<node_internals>/**", "./built/**/*"]
  }
  